<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.giriasestadosbrasileiros" version="1.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Gírias dos Estados Brasileiros</name>
    <description>
        App que exibe as gírias dos estados brasileiros.
    </description>
    <author email="miqueiasmcaetano@gmail.com" href="https://www.innovatesoft.com.br/">
        Innovate Soft
    </author>
    <content src="index.html" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.4.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" source="npm" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.3" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~2.0.2" />
    <plugin name="cordova-clipboard" source="npm" />
    <preference name="windows-appx-target" value="8.1-phone" />
    <preference name="WindowsToastCapable" value="true" />
    <access origin="*" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#006400" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="29" />
    <splash src="./www/img/Default@2x~universal~anyany.png" />
    <splash src="./www/img/Default@2x~universal~comany.png" />
    <splash src="./www/img/Default@2x~universal~comcom.png" />
    <splash src="./www/img/Default@3x~universal~anyany.png" />
    <splash src="./www/img/Default@3x~universal~anycom.png" />
    <splash src="./www/img/Default@3x~universal~comany.png" />
    <platform name="android">
        <icon density="ldpi" src="./www/img/doctum800x800.png" />
        <icon density="mdpi" src="./www/img/doctum800x800.png" />
        <icon density="hdpi" src="./www/img/doctum800x800.png" />
        <icon density="xhdpi" src="./www/img/doctum800x800.png" />
        <icon density="xxhdpi" src="./www/img/doctum800x800.png" />
        <icon density="xxxhdpi" src="./www/img/doctum800x800.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <plugin name="cordova-plugin-admob-free" spec="0.27.0">
        <variable name="ADMOB_APP_ID" value="ca-app-pub-7091486462236476~9915980012" />
    </plugin>
    <plugin name="cordova-plugin-x-socialsharing" spec="5.6.4" />
</widget>
