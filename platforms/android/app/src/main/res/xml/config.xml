<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.giriasestadosbrasileiros" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OneSignalPush">
        <param name="android-package" value="com.plugin.gcm.OneSignalPush" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" value="org.apache.cordova.statusbar.StatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.cupertino.cordova.plugin.AdMob" />
        <param name="onload" value="true" />
    </feature>
    <name>Gírias dos Estados Brasileiros</name>
    <description>
        App que exibe as gírias dos estados brasileiros.
    </description>
    <author email="miqueiasmcaetano@gmail.com" href="https://miqueiasmcaetano.000webhostapp.com/">
        Innovate Soft
    </author>
    <content src="index.html" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.4.1" />
    <gap:plugin name="cordova-admob-sdklibs" source="npm" spec="^2.1.6" />
    <gap:plugin name="cordova-plugin-admob-simple" source="npm" spec="^3.3.8" />
    <splash src="./www/img/Default@2x~universal~anyany.png" />
    <splash src="./www/img/Default@2x~universal~comany.png" />
    <splash src="./www/img/Default@2x~universal~comcom.png" />
    <splash src="./www/img/Default@3x~universal~anyany.png" />
    <splash src="./www/img/Default@3x~universal~anycom.png" />
    <splash src="./www/img/Default@3x~universal~comany.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" src="./www/img/doctum800x800.png" />
    <icon density="mdpi" src="./www/img/doctum800x800.png" />
    <icon density="hdpi" src="./www/img/doctum800x800.png" />
    <icon density="xhdpi" src="./www/img/doctum800x800.png" />
    <icon density="xxhdpi" src="./www/img/doctum800x800.png" />
    <icon density="xxxhdpi" src="./www/img/doctum800x800.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="windows-appx-target" value="8.1-phone" />
    <preference name="WindowsToastCapable" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#006400" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="16" />
</widget>
